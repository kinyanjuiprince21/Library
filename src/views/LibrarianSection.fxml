<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LibrarianSection">
   <children>
      <Label alignment="CENTER" prefHeight="18.0" prefWidth="600.0" text="models.Librarian Section">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#addBooks" text="Add Books" />
      <Button mnemonicParsing="false" onAction="#viewBooks" text="View Books" />
      <Button mnemonicParsing="false" onAction="#issueBook" text="Issue Books" />
      <Button mnemonicParsing="false" onAction="#viewIssuedBook" text="View Issued Books" />
      <Button mnemonicParsing="false" onAction="#returnBook" text="Return Book" />
      <Button fx:id="toLibrarySection" mnemonicParsing="false" onAction="#toLogInForm" text="LogOut" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" top="20.0" />
   </padding>
</VBox>
