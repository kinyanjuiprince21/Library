<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ViewLibrarianController">
   <children>
      <HBox>
         <children>
            <Button mnemonicParsing="false" onAction="#backAdminSection" text="Back" />
         </children>
         <padding>
            <Insets bottom="2.0" left="20.0" top="2.0" />
         </padding>
      </HBox>
      <TableView fx:id="tblViewLibrarian" prefHeight="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="id" prefWidth="100.0" text="id" />
          <TableColumn fx:id="name" prefWidth="150.0" text="name" />
            <TableColumn fx:id="password" prefWidth="200.0" text="password" />
            <TableColumn fx:id="email" prefWidth="200.0" text="email" />
            <TableColumn fx:id="address" prefWidth="200.0" text="address" />
            <TableColumn fx:id="city" prefWidth="100.0" text="city" />
            <TableColumn fx:id="contact" prefWidth="150.0" text="contact" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
