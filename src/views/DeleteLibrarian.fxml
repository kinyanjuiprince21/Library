<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="190.0" prefWidth="453.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DeleteLibrarianController">
   <children>
      <HBox>
         <children>
            <Label prefHeight="16.0" prefWidth="76.0" text="Enter Id:" />
            <TextField fx:id="txtId" />
            <Label fx:id="lblRequired" text="Label" />
         </children>
      </HBox>
      <HBox spacing="30.0">
         <children>
            <Button fx:id="btnDelete" mnemonicParsing="false" text="Delete">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#backToAdminSection" text="Back">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="30.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="40.0" left="20.0" right="20.0" top="40.0" />
   </padding>
</VBox>
